Public Function MakeARGB(ByVal a As Byte, ByVal r As Byte, ByVal g As Byte, ByVal b As Byte) As Long
	return ((b) << BlueShift) Or _
                ((g) << GreenShift) Or _
                ((r) <<   RedShift) Or _
                ((a) << AlphaShift)
End Function

