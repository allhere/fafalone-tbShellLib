Module slDiskQuota

Public Enum DiskQuotaStates
	DISKQUOTA_STATE_DISABLED0x00000000 = 0
	DISKQUOTA_STATE_TRACK0x00000001 = 1
	DISKQUOTA_STATE_ENFORCE0x00000002 = 2
	DISKQUOTA_STATE_MASK0x00000003 = 3
	DISKQUOTA_FILESTATE_INCOMPLETE0x00000100 = 4
	DISKQUOTA_FILESTATE_REBUILDING0x00000200 = 5
	DISKQUOTA_FILESTATE_MASK0x00000300 = 6
End Enum

Public Enum DiskQuotaLogFlags
	DISKQUOTA_LOGFLAG_USER_THRESHOLD0x00000001 = 0
	DISKQUOTA_LOGFLAG_USER_LIMIT0x00000002 = 1
End Enum

Public Type DISKQUOTA_USER_INFORMATION
	QuotaUsed As LONGLONG
	QuotaThreshold As LONGLONG
	QuotaLimit As LONGLONG
End Type

Public Enum DiskQuotaNameResolution
	DISKQUOTA_USERNAME_RESOLVE_NONE0 = 0
