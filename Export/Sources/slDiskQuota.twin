End Enum

Public Enum DiskQuotaAccountStatus
	DISKQUOTA_USER_ACCOUNT_RESOLVED0 = 0
	DISKQUOTA_USER_ACCOUNT_UNAVAILABLE1 = 1
	DISKQUOTA_USER_ACCOUNT_DELETED2 = 2
	DISKQUOTA_USER_ACCOUNT_INVALID3 = 3
	DISKQUOTA_USER_ACCOUNT_UNKNOWN4 = 4
	DISKQUOTA_USER_ACCOUNT_UNRESOLVED5 = 5
End Enum

End Module